{

  "name": "@tillpos/rn-receipt-printer-utils",
  "version": "0.1.1",
  "description": "Fork of react-native-printer. A React Native Library to support USB/BLE/Net printer",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "author": "Till",
  "homepage":"https://github.com/hitz-group/rn-receipt-printer-utils.git",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/hitz-group/rn-receipt-printer-utils.git"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "tillpos-rn-receipt-printer-utils.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hitz-group/rn-receipt-printer-utils/issues"
  },
  "keywords": [
    "react-native",
    "thermal",
    "printer",
    "bluetooth",
    "net",
    "printer",
    "rn",
    "android"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "commitlint": "^11.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "release-it": "^14.2.2",
    "@types/node": "^14.14.20",
    "react": "^16.13.1",
    "react-native": "^0.63.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "ts-loader": "^2.3.4",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "react-native-builder-bob": "^0.17.1",
    "@types/react-native": "0.62.13"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
