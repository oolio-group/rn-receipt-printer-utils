{

  "name": "@tillpos/rn-receipt-printer-utils",
  "version": "0.0.10",
  "description": "Fork of react-native-printer. A React Native Library to support USB/BLE/Net printer",
  "main": "lib/index",
  "source": "lib/index",
  "author": "Till",
  "homepage":"https://github.com/hitz-group/rn-receipt-printer-utils.git",
  "scripts": {
    "pods": "cd example && pod-install --quiet",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn && yarn pods",
    "clean": "rm -rf ./lib && rm -rf tsconfig.build.tsbuildinfo",
    "build": "yarn  clean && yarn  compile",
    "compile": "tsc -p ./tsconfig.build.json"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/hitz-group/rn-receipt-printer-utils.git"
  },
  "files": [
    "lib",
    "android",
    "ios",
    "tillpos-rn-receipt-printer-utils.podspec"
  ],
  "types": "lib",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hitz-group/rn-receipt-printer-utils/issues"
  },
  "keywords": [
    "react-native",
    "thermal",
    "printer",
    "bluetooth",
    "net",
    "printer",
    "rn",
    "android"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "@types/node": "^14.14.20",
    "pod-install": "^0.1.10",
    "react": "^16.13.1",
    "react-native": "^0.63.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "ts-loader": "^2.3.4",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1"
  }
}
